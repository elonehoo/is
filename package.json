{
  "name": "@titem/monorepo",
  "private": "true",
  "description": "Type check values",
  "author": "Elone Hoo <huchengyea@163.com>",
  "type": "module",
  "license": "MIT",
  "version": "0.0.2",
  "scripts": {
    "release": "bumpp package.json core/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "pnpm -C core run build",
    "test": "pnpm -C test run test",
    "coverage": "pnpm -C test run coverage",
    "reporter": "pnpm -C test run reporter"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@elonehoo/is": "workspace:*",
    "@types/node": "18.15.11",
    "bumpp": "9.1.0",
    "eslint": "8.38.0",
    "pnpm": "7.32.0",
    "rimraf": "4.4.1",
    "typescript": "4.9.5",
    "unbuild": "1.2.0",
    "vite": "4.2.1",
    "vitest": "0.30.0"
  }
}
